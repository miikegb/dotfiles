# This a list of useful shortcuts that make my life easier at work.
alias pu="pod update"
alias up="cd .."
alias pull="git pull"
alias shipit="git push"
alias lg="git lg"
alias changes="lg -p"
alias c="clear"

alias l="ls -la"
alias launchfrank="ios-sim launch Frank/frankified_build/Frankified.app --devicetypeid "com.apple.CoreSimulator.SimDeviceType.iPhone-5, 8.1" --exit"

alias xc="find . -depth 1 -name '*.xcworkspace' -exec open -a '/Applications/Xcode.app' {} \;"
alias xcb="find . -depth 1 -name '*.xcworkspace' -exec open -a '/Applications/Xcode-beta.app' {} \;"

function edit_shortcuts {
  atom ~/Dev/dotfiles/.shortcuts
}
