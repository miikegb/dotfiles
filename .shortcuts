# This a list of useful shortcuts that make my life easier at work.

REPOS_DIR="~/Documents/Dev"

alias pu="pod update"
alias rl="cd $REPOS_DIR/remotelink/RemoteLink"
alias base="cd $REPOS_DIR/onstarbase/OnStarBase"
alias services="cd $REPOS_DIR/onstarservices/OnStarServices"
alias auth="cd $REPOS_DIR/onstarauthentication/OnStarAuthentication"
alias maps="cd $REPOS_DIR/onstartelenavmaps"
alias analytics="cd $REPOS_DIR/onstaranalytics/OnStarAnalytics"
alias frankify="frank build --workspace RemoteLink.xcworkspace --scheme RemoteLink"
alias rf="./Scripts/run_frank.sh"
alias unit="./Scripts/unit_tests.sh"
alias cut_new_version="./Scripts/podspec_update.sh"
alias up="cd .."
alias pull="git pull"
alias shipit="git push"
alias lg="git lg"
alias changes="lg -p"
alias c="clear"
alias reload=". $REPOS_DIR/dotfiles/.shortcuts"
alias fix_charles="~/Documents/Stuff/install-charles-ca-cert-for-iphone-simulator.command"
alias l="ls -la"
alias hifrank="ios-sim launch Frank/frankified_build/Frankified.app --devicetypeid "com.apple.CoreSimulator.SimDeviceType.iPhone-5, 8.1" --exit"

export ONSTAR_SERVICES="$REPOS_DIR/onstarservices/OnStarServices"
export ONSTAR_ANALYTICS="$REPOS_DIR/onstaranalytics/OnStarAnalytics"

function start_servers {
  ./Scripts/start_fake_server.sh
  ./Scripts/start_legal_server.sh
}

function edit_shortcuts {
  atom ~/Documents/Dev/dotfiles/.shortcuts
}
